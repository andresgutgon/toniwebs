---
import Layout from '@layouts/Default.astro'
import Home from '@PageComponents/Weedings/index.astro'
import { site, getPage } from '@site/index'

export function getStaticPaths() {
  return [
    { params: { locale: 'ca'} },
  ]
}

const { locale } = Astro.params
const page = getPage(site, 'home', locale)
---
<Layout page={page}><Home page={page} /></Layout>
