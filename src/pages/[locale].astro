---
import Layout from '@layouts/Default.astro'
import Home from '@PageComponents/Weedings/index.astro'
import { sites, getPage } from '@site/index'

export function getStaticPaths() {
  return [
    { params: { locale: 'ca'} },
  ]
}

const { locale } = Astro.params
const site = getPage(sites.maestroCeremoniasCat, 'home', locale)
---
<Layout site={site}><Home page={page} /></Layout>
