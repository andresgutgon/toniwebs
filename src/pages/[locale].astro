---
import Layout from '@layouts/Default.astro'
import Home from '@components/MaestroCeremonias/Home/index.astro'
import { sites, getPage } from '@sites/index'

export function getStaticPaths() {
  return [
    { params: { locale: 'ca'} },
  ]
}

const { locale } = Astro.params
const site = getPage(sites.maestroCeremoniasCat, 'home', locale)
---
<Layout site={site}><Home site={site} /></Layout>
