{
  "name": "@example/minimal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "AstroBuild": "astro build",
    "preview": "astro preview",
    "tc": "tsc --noEmit",
    "extract:i18n": "formatjs extract 'src/{pages,PageComponents,components,layouts}/**/*.{js,ts,tsx}' --ignore='**/*.d.ts' --out-file 'src/locales/es.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile:i18n": "formatjs compile-folder --ast src/locales src/locales/compiled",
    "i18n": "npm run extract:i18n && npm run compile:i18n",
    "build": "npm run i18n && npm run AstroBuild"
  },
  "devDependencies": {
    "@astrojs/react": "^0.1.3",
    "@astrojs/sitemap": "^0.2.4",
    "@astrojs/tailwind": "^0.2.1",
    "@formatjs/cli": "^5.0.2",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "astro": "^1.0.0-beta.44",
    "astro-seo": "^0.5.0",
    "classnames": "^2.3.1",
    "prettier": "2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "react-intl": "^6.0.5"
  }
}
